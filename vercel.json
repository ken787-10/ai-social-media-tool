{
  "functions": {
    "api/webhook.ts": {
      "maxDuration": 10
    },
    "api/admin/path.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/api/admin/:path*",
      "destination": "/api/admin/path?path=$path"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}